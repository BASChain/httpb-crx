{
  "name": "httpb-crx",
  "version": "0.0.41",
  "description": "The BAS Extendsion for chromium",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development gulp:build",
    "chrome": "web-ext run -t chromium --browser-console",
    "firefox": "web-ext run -t firefox --browser-console",
    "build-release": "cross-env NODE_ENV=production gulp:build",
    "gulp:build": "npx env-cmd -r ./.config/.env-cmdrc -e dev,comm gulp default",
    "test": "npx DEV_MODE=ture env-cmd -r ./.config/.env-cmdrc -e comm node test/devtools/test.js",
    "deps-install": "npx yarn@1.21.0 && npx yarn@1.21.0 install"
  },
  "repository": {
    "type": "git",
    "url": "git@flash:BASChain/httpb-crx.git"
  },
  "keywords": [
    "Blockchain",
    "DNS"
  ],
  "author": "basFronter",
  "license": "MIT",
